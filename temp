  function displayNote(temp, condition){
    var note;
    if(condition == "Clear" && temp < "85"){
        note = "Don't forget sunscreen!";
    }
    else if(condition == "Rain"){
        note = "Better grab your umbrella...";
    }
    else if(condition == "Snow"){
        note = "Time to get that shovel ready!";
    }
    else{
        note = " ";
    }

    document.getElementById("weather-text").innerHTML = note;
}